{
  "name": "dito-example",
  "version": "0.4.0",
  "description": "",
  "main": "src/server/app.js",
  "repository": "https://github.com/ditojs/dito-example",
  "author": "JÃ¼rg Lehni <juerg@scratchdisk.com> (http://scratchdisk.com)",
  "license": "MIT",
  "scripts": {
    "db:create_migration": "dito db:create_migration src/server/app",
    "db:migrate": "dito db:migrate src/config",
    "db:rollback": "dito db:rollback src/config",
    "db:reset": "dito db:reset src/config",
    "db:seed": "dito db:seed src/server/app",
    "console": "dito console src/server/app",
    "start": "babel-node src/server",
    "watch": "nodemon --watch src/server --watch src/config --exec 'yarn start'",
    "watch:dito": "nodemon --watch src/server --watch src/config --watch ../../dito/packages/server/lib  --watch ../../dito/packages/utils/lib --watch ../../dito/packages/router/lib --watch ../../objection.js --watch ../../knex --exec 'npm run start'",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "dev:dito": "cross-env NODE_ENV=development npm run watch:dito",
    "debug": "cross-env DEBUG=* yarn start",
    "inspect": "babel-node --inspect --debug-brk src/server",
    "build:server": "babel src/server -d dist/server --copy-files --source-maps inline",
    "build:admin": "cross-env NODE_ENV=production vue-cli-service build src/admin/index.js --dest dist/admin",
    "build": "rm -rf dist && mkdir dist && npm run build:server && npm run build:admin",
    "serve": "node dist/server",
    "lint": "cross-env NODE_ENV=production eslint src test migrations seeds",
    "lint:fix": "eslint --fix src test migrations seeds",
    "precommit": "yarn lint"
  },
  "engines": {
    "node": ">= 10.0.0",
    "yarn": ">= 1.0.0"
  },
  "dependencies": {
    "@ditojs/server": "^0.150.0",
    "@ditojs/utils": "^0.150.0",
    "bluebird": "^3.7.2",
    "core-js": "^3.6.5",
    "current-script-polyfill": "^1.0.0",
    "knex": "^0.21.2",
    "koa-static": "^5.0.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@ditojs/babel-preset": "^0.150.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-promise-to-bluebird": "^2.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4"
  }
}
