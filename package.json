{
  "name": "dito-example",
  "version": "0.4.0",
  "description": "",
  "main": "src/server/app.js",
  "repository": "https://github.com/ditojs/dito-example",
  "author": "JÃ¼rg Lehni <juerg@scratchdisk.com> (http://scratchdisk.com)",
  "license": "MIT",
  "scripts": {
    "db:create_migration": "dito db:create_migration src/server/app",
    "db:migrate": "dito db:migrate src/config",
    "db:rollback": "dito db:rollback src/config",
    "db:reset": "dito db:reset src/config",
    "db:seed": "dito db:seed src/server/app",
    "console": "dito console src/server/app",
    "start": "babel-node src/server",
    "watch": "nodemon --watch src/server --watch src/config --exec 'yarn start'",
    "watch:dito": "nodemon --watch src/server --watch src/config --watch ../dito/packages/server/lib  --watch ../dito/packages/utils/lib --watch ../dito/packages/router/lib --watch ../objection.js --watch ../knex --exec 'yarn start'",
    "dev": "cross-env NODE_ENV=development yarn watch",
    "dev:dito": "cross-env NODE_ENV=development yarn watch:dito",
    "debug": "cross-env DEBUG=* yarn start",
    "inspect": "babel-node --inspect --debug-brk src/server",
    "clean": "rimraf dist",
    "build:src": "babel src -d dist --copy-files --source-maps inline",
    "build:admin": "cross-env NODE_ENV=production vue-cli-service build src/admin/index.js --dest dist/admin",
    "build": "yarn build:src && yarn build:admin",
    "serve": "node dist/server",
    "lint": "cross-env NODE_ENV=production eslint src test migrations seeds",
    "lint:fix": "eslint --fix src test migrations seeds",
    "prepare": "yarn clean && yarn build",
    "precommit": "yarn lint"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 1.0.0"
  },
  "dependencies": {
    "@ditojs/server": "^0.193.0",
    "@ditojs/utils": "^0.193.0",
    "core-js": "^3.6.5",
    "knex": "^0.21.12",
    "koa-static": "^5.0.0",
    "node-sass": "^5.0.0",
    "objection": "^2.2.3",
    "sass-loader": "^10.0.5",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/eslint-plugin": "^7.11.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@ditojs/babel-preset": "^0.193.0",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}
